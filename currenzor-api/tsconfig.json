{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "lib": [
            "esnext",
            "esnext.asynciterable"
        ],
        "sourceMap": true,
        "watch": false,
        "outDir": "dist",
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "suppressImplicitAnyIndexErrors": true,
        "noUnusedLocals": true,
        "allowSyntheticDefaultImports": true
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}